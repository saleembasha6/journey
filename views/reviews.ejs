<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Reviews</title>
</head>
<body>
    <h1>Reviews</h1>
    <div id="reviews">
        <% reviews.forEach(review => { %>
            <div class="review">
                <h2><%= review.name %></h2>
                <p><strong>Rating:</strong> <%= review.rating %></p>
                <p><%= review.comment %></p>
                <hr>
            </div>
        <% }) %>
    </div>
</body>
</html>
